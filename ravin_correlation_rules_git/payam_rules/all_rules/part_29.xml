                <message>PLC ALARAM initiate command</message>
                <source>
                    <ip reference_id="1" reference_type="source" />
                </source>
                <target>
                    <ip reference_id="1" reference_type="target" />
                </target>
                <additional_data meaning="Function Group" reference_id="1" />
                <additional_data meaning="Subfunction" reference_id="1" />
            </alert>
        </action>
        <regulation>
            <sequence>
                <filter id="1" />
            </sequence>
        </regulation>
    </rule>
    