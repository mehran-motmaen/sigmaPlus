                    <ip location="ANY" />
                    <port>0-65535</port>
                </source>
                <target>
                    <ip location="ANY" />
                    <port>0-65535</port>
                </target>
                <event_list>
                    <event sensor_type="3" event_type="1025:63" />
                    <event sensor_type="3" event_type="1025:67" />
                    <event sensor_type="3" event_type="1:63" />
                    <event sensor_type="3" event_type="1:67" />
                </event_list>
            </filter>
        </filter_list>
        <action>
