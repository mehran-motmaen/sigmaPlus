<rule id="3903" owner="system" enable="f" kbid="3903_1" ver="1" file_name="">
        <filter_list>
            <filter id="1" name="User Web Page Redirection made By Spyware/Trojan Detected" weight="20">
                <source>
                    <ip location="ANY" />
                    <port>0-65535</port>
                </source>
                <target>
                    <ip location="ANY" />
                    <port>0-65535</port>
                </target>
                <event_list>
                    <event sensor_type="ANY" class_type="">
                        <tag meaning="event_type">Malware</tag>
                        <tag meaning="mechanism_detail">Redirection</tag>
                    </event>
                </event_list>
            </filter>
            <filter id="2" name="User Web Page Redirection made By Spyware/Trojan Detected" occurrence="2" weight="5" timeout="120">
                <source>
                    <ip reference_id="1" reference_type="source" />
                    <port>0-65535</port>
                </source>
                <target>
                    <ip reference_id="1" reference_type="target" />
                    <port>0-65535</port>
                </target>
                <event_list>
                    <event sensor_type="ANY" class_type="">
                        <tag meaning="event_type">Malware</tag>
                        <tag meaning="mechanism_detail">Redirection</tag>
                    </event>
                </event_list>
            </filter>
            <filter id="3" name="User Web Page Redirection made By Spyware/Trojan Detected" occurrence="5" weight="10" timeout="7200">
                <source>
                    <ip reference_id="1" reference_type="source" />
                    <port>0-65535</port>
                </source>
                <target>
                    <ip reference_id="1" reference_type="target" />
                    <port>0-65535</port>
                </target>
                <event_list>
                    <event sensor_type="ANY" class_type="">
                        <tag meaning="event_type">Malware</tag>
                        <tag meaning="mechanism_detail">Redirection</tag>
                    </event>
                </event_list>
            </filter>
        </filter_list>
        <action>
            <alert priority="medium" category="Masquerade">
                <message>A Trojan/Spyware Aplication Redirect User Web Page</message>
                <source>
                    <ip reference_id="1" reference_type="source" />
                </source>
                <target>
                    <ip reference_id="1" reference_type="target" />
                </target>
                <additional_data meaning="MalwareName" reference_id="1" />
                <additional_data meaning="URL" reference_id="1" />
            </alert>
            <prerequisite>
                <predicate type="Hopping">
                    <param reference_type="source.ip" />
                </predicate>
                <predicate type="Trojan-Host">
                    <param reference_type="source.ip" />
                </predicate>
                <predicate type="Infection-Malicious">
                    <param reference_type="source.ip" />
                </predicate>
            </prerequisite>
            <consequence>
                <predicate type="Trojan-Host">
                    <param reference_type="source.ip" />
                </predicate>
                <predicate type="Trojan-Server">
                    <param reference_type="target.ip" />
                </predicate>
                <predicate type="Compromise-Host">
                    <param reference_type="source.ip" />
                </predicate>
                <predicate type="Hopping" timeout="43200">
                    <param reference_type="target.ip" />
                </predicate>
            </consequence>
        </action>
        <regulation>
            <sequence>
                <filter id="1" />
                <filter id="2" />
                <filter id="3" />
            </sequence>
        </regulation>
    </rule>
    