<rule id="6104" owner="system" kbid="6104_1" enable="t" ver="1" file_name="">
        <filter_list>
            <filter id="1" name="Unauthorized Database Access Attempt Detection" weight="30">
                <source>
                    <ip location="ANY" />
                    <port>0-65535</port>
                </source>
                <target>
                    <ip location="ANY" />
                    <port>0-65535</port>
                </target>
                <event_list>
                    <event sensor_type="3">
                        <tag meaning="mechanism">Authorization</tag>
                        <tag meaning="result">Failed</tag>
                        <tag meaning="resource">DBMS</tag>
                    </event>
                </event_list>
            </filter>
        </filter_list>
        <action>
            <alert priority="high" category="AttemptUser">
                <message>Unauthorized attempt to access database detected</message>
                <source>
                    <ip reference_id="1" reference_type="source" />
                </source>
                <target>
                    <ip reference_id="1" reference_type="target" />
                </target>
            </alert>
            <prerequisite />
            <consequence />
        </action>
        <regulation>
            <sequence>
                <filter id="1" />
            </sequence>
        </regulation>
    </rule>
    