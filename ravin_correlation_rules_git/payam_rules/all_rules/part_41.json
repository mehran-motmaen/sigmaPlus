          "event_list": {
            "event": {
              "@sensor_type": "1",
              "@class_type": "",
              "tag": [
                {
                  "@meaning": "mechanism",
                  "#text": "BadIP Inbound Traffic"
                },
                {
                  "@meaning": "resource",
                  "#text": "Proxy"
                },
                {
                  "@meaning": "event_type",
                  "#text": "Suspicious Activity"
                }
              ]
            }
          }
        },
        {
          "@id": "2",
          "@name": "Login authentication detected",
          "@occurrence": "1",
          "@weight": "10",
          "@timeout": "300",
          "source": {
            "ip": {
              "@reference_id": "1",
