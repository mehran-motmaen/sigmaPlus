        "@priority": "medium",
        "@category": "Botnet",
        "message": "Suspicious login attempt from Tor on SRC_IP",
        "source": {
          "ip": {
            "@reference_id": "1",
            "@reference_type": "source"
          }
        },
        "target": {
          "ip": {
            "@reference_id": "1",
            "@reference_type": "target"
          }
        },
        "additional_data": {
          "@meaning": "Username",
