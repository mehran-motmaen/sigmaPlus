<rule kbid="7012_1" id="7012" owner="system" ver="1" enable="t" file_name="">
        <filter_list>
            <filter id="1" name="Waste of Resource detected" weight="5">
                <source>
                    <ip location="ANY" />
                    <port>0-65535</port>
                </source>
                <target>
                    <ip location="ANY" />
                    <port>0-65535</port>
                </target>
                <event_list>
                    <event sensor_type="ANY">
                        <tag meaning="event_type">Information Status</tag>
                        <tag meaning="effect">Degradation</tag>
                    </event>
                </event_list>
            </filter>
        </filter_list>
        <action>
            <alert priority="medium" category="WasteOfResource">
                <message>Waste of Resource on a Network Device Detected</message>
                <source>
                    <ip reference_id="1" reference_type="source" />
                </source>
                <target>
                    <ip reference_id="1" reference_type="target" />
                </target>
            </alert>
        </action>
        <regulation>
            <sequence>
                <filter id="1" />
            </sequence>
        </regulation>
    </rule>
    