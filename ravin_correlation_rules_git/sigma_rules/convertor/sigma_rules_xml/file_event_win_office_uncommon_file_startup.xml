<?xml version="1.0" encoding="UTF-8" ?><root><title type="str">Uncommon File Created In Office Startup Folder</title><id type="str">a10a2c40-2c4d-49f8-b557-1a946bc55d9d</id><status type="str">experimental</status><description type="str">Detects the creation of a file with an uncommon extension in an Office application startup folder</description><references type="list"><item type="str">https://app.any.run/tasks/d6fe6624-6ef8-485d-aa75-3d1bdda2a08c/</item><item type="str">http://addbalance.com/word/startup.htm</item><item type="str">https://answers.microsoft.com/en-us/msoffice/forum/all/document-in-word-startup-folder-doesnt-open-when/44ab0932-2917-4150-8cdc-2f2cf39e86f3</item><item type="str">https://en.wikipedia.org/wiki/List_of_Microsoft_Office_filename_extensions</item></references><author type="str">frack113, Nasreddine Bencherchali (Nextron Systems)</author><date type="str">2022/06/05</date><modified type="str">2023/06/23</modified><tags type="list"><item type="str">attack.resource_development</item><item type="str">attack.t1587.001</item></tags><logsource type="dict"><product type="str">windows</product><category type="str">file_event</category></logsource><detection type="dict"><selection_word_paths type="list"><item type="dict"><key name="TargetFilename|contains" type="str">\Microsoft\Word\STARTUP</key></item><item type="dict"><key name="TargetFilename|contains|all" type="list"><item type="str">\Office</item><item type="str">\Program Files</item><item type="str">\STARTUP</item></key></item></selection_word_paths><filter_exclude_word_ext type="dict"><key name="TargetFilename|endswith" type="list"><item type="str">.docb</item><item type="str">.docm</item><item type="str">.docx</item><item type="str">.dotm</item><item type="str">.pdf</item><item type="str">.wll</item><item type="str">.wwl</item></key></filter_exclude_word_ext><selection_excel_paths type="list"><item type="dict"><key name="TargetFilename|contains" type="str">\Microsoft\Excel\XLSTART</key></item><item type="dict"><key name="TargetFilename|contains|all" type="list"><item type="str">\Office</item><item type="str">\Program Files</item><item type="str">\XLSTART</item></key></item></selection_excel_paths><filter_exclude_excel_ext type="dict"><key name="TargetFilename|endswith" type="list"><item type="str">.xls</item><item type="str">.xlsm</item><item type="str">.xlsx</item><item type="str">.xlt</item><item type="str">.xltm</item><item type="str">.xll</item><item type="str">.xlw</item></key></filter_exclude_excel_ext><filter_main_office_click_to_run type="dict"><key name="Image|startswith" type="str">C:\Program Files\Common Files\Microsoft Shared\ClickToRun\</key><key name="Image|endswith" type="str">\OfficeClickToRun.exe</key></filter_main_office_click_to_run><filter_main_office_apps type="dict"><key name="Image|startswith" type="list"><item type="str">C:\Program Files\Microsoft Office\</item><item type="str">C:\Program Files (x86)\Microsoft Office\</item></key><key name="Image|endswith" type="list"><item type="str">\winword.exe</item><item type="str">\excel.exe</item></key></filter_main_office_apps><condition type="str">((selection_word_paths and not filter_exclude_word_ext) or (selection_excel_paths and not filter_exclude_excel_ext)) and not 1 of filter_main_*</condition></detection><falsepositives type="list"><item type="str">Other legitimate extensions used by either excel or word</item></falsepositives><level type="str">high</level></root>